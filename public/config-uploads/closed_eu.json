{
	"closed_eu":{
		"category_scrape": {
			"allowed_domains": ["www.closed.com"],
			"category_start_urls": {
				"women_clothes": ["https://www.closed.com/en/women/"],
				"men_clothes": ["https://www.closed.com/en/men/"],
				"children_clothes": [""]
			},
			"category_links": {
				"women_clothes": ["xpath", "//*[@id]/li[3]/div/ul[position()<last()]/li/a/@href", ""],
				"men_clothes": ["xpath", "//*[@id]/li[5]/div/ul[position()<last()]/li/a/@href", ""],
				"children_clothes": ["", "", ""]
			},
			"category_names": {
				"women_clothes": ["xpath", "//*[@id]/li[3]/div/ul[position()<last()]/li/a/span/text()", ""],
				"men_clothes": ["xpath", "//*[@id]/li[5]/div/ul[position()<last()]/li/a/span/text()", ""],
				"children_clothes": ["", "", ""]
			},
			"category_pagination": ["multi-load", "class", "#itemsPagerbottom li:nth-last-child(2) a span", "_text", "?pgNr="],
			"pagination_parameters": {
				"step_size": 1, 
				"start_index": 1, 
				"last_page": -1
			},
			"category_harvest_data": {
				"all_selectors_type": "xpath",
				"data_parent": ["//*[@id]/li/form/div", ""],
				"product_name": ["//h2/span[last()]/text()", ""],
				"product_link": ["//a/@href", ""],
				"normal_price": ["//div/span[1]/meta[1]/@content", ""],
				"old_price": ["//div/span[last()]/del/text()", ""]
			}
		},
		"product_scrape": {
			"name": ["class", ".productdetails__title.h3", "_text"],
			"color": ["class", ".d-ib.js-pdp-color-selected", "_text"],
			"available_size": ["class", ".productdetails__sizes li:not(.disabled):not(.productdetails__size-link) a", "_text"],
			"unavailable_size": ["class", ".js-disabled.disabled a del", "_text"]
		},
		"currency": "EUR"
	}
}